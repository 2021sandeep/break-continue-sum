{
    int a=10,b=5;
    char ch;
    
    printf("enter a charecter:");
    scanf("%c",&ch);
    switch(ch)
    {
               case 'a':
               a+=b;
               printf("%d sum is",a);
               break;
               case 's':
               a-=b;
               printf("%d differnce is",a);
               break;
               default:
               printf("enter valide charecters");
    }
    
}

#include<stdio.h>
void main()
{
    int i,j;
    printf("even numbers 1-100-:");
    for(i=1;i<=10;i++)
    {
        if(i%2==0)
        {
            printf("%d",i);
        }
    }
    printf("\nodd numbers 1-100-:");
    for(j=0;j<=10;j++)
    {
        if(j%2!=0)
        {
            printf("%d",j);
        }
